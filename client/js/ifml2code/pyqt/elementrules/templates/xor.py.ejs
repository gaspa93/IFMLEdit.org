def setupUi():

    w = QtGui.QWidget()
    w.setGeometry(100,100,700,600)
    w.setWindowTitle("<%=id %>")

    tabWidget = QtGui.QTabWidget(w)
    tabWidget.setGeometry(30, 30, 621, 531)

<% for (var i = 0; i < children.length; i++) { -%>
    tab = QtGui.QWidget()
    tab.setGeometry(10, 10, 601, 471)
    tab.setObjectName("<%=children[i].id %>")
    tabWidget.addTab(tab)
<% } -%>

    return w
